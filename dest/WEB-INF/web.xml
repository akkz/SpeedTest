<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID"
	version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" >
	<display-name>AhauJoin </display-name>
	<welcome-file-list>
		<welcome-file>quickTest.html</welcome-file>
	</welcome-file-list>
  
  <servlet>
	<servlet-name>dataSend</servlet-name>
	<servlet-class>akkz.DataSend</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>dataSend</servlet-name>
	<url-pattern>/dataSend.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>pressTest</servlet-name>
	<servlet-class>akkz.PressTest</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>pressTest</servlet-name>
	<url-pattern>/pressTest.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>State</servlet-name>
	<servlet-class>akkz.State</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>State</servlet-name>
	<url-pattern>/state.do</url-pattern>
  </servlet-mapping>
</web-app>
